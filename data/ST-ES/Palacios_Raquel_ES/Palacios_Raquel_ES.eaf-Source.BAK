	<TIER DEFAULT_LOCALE="ca" LINGUISTIC_TYPE_REF="utterance" TIER_ID="AD-en">
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a20" TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
				<ANNOTATION_VALUE>Es Rick, el jubilado.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a21" TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
				<ANNOTATION_VALUE>Está en un parque. Deja la tapa en la silla de al lado y toma otro trago.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a22" TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
				<ANNOTATION_VALUE>Mueve una pierna con un tic nervioso y deja el vaso.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a23" TIME_SLOT_REF1="ts45" TIME_SLOT_REF2="ts46">
				<ANNOTATION_VALUE>Saca el móvil del bolsillo, elige un contacto de la lista y llama.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a24" TIME_SLOT_REF1="ts47" TIME_SLOT_REF2="ts48">
				<ANNOTATION_VALUE>Frunce el ceño y levanta la cabeza.  </ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a25" TIME_SLOT_REF1="ts49" TIME_SLOT_REF2="ts50">
				<ANNOTATION_VALUE>Rick se aleja mientras intenta localizar la procedencia del pitido.  El vaso de café queda abandonado en la silla.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a26" TIME_SLOT_REF1="ts51" TIME_SLOT_REF2="ts52">
				<ANNOTATION_VALUE>Jess, la estudiante, es una adolescente de rizos pelirrojos.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a27" TIME_SLOT_REF1="ts53" TIME_SLOT_REF2="ts54">
				<ANNOTATION_VALUE>Jess pasea la mirada por la habitación.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a28" TIME_SLOT_REF1="ts55" TIME_SLOT_REF2="ts56">
				<ANNOTATION_VALUE>Se pone el abrigo y sale a la calle.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a29" TIME_SLOT_REF1="ts57" TIME_SLOT_REF2="ts58">
				<ANNOTATION_VALUE>Sube un camino cubierto de maleza y se pierde en la espesura.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a17" TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
				<ANNOTATION_VALUE>Sentado en una silla hay un hombre de unos 50 años.  Tiene el pelo gris y revuelto y una barba descuidada.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a16" TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
				<ANNOTATION_VALUE>Asciende por una ladera de tierra sembrada de arbustos.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a11" TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
				<ANNOTATION_VALUE>Se queda quieto un instante.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a10" TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
				<ANNOTATION_VALUE>Luego camina por la playa en busca del origen del pitido.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a33" TIME_SLOT_REF1="ts65" TIME_SLOT_REF2="ts66">
				<ANNOTATION_VALUE>Los tres niegan con la cabeza.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a13" TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
				<ANNOTATION_VALUE>Vuelve a la toalla con los ojos entornados y el ceño fruncido.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a12" TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
				<ANNOTATION_VALUE>Camina hacia el lado contrario mientras escruta los alrededores.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a32" TIME_SLOT_REF1="ts63" TIME_SLOT_REF2="ts64">
				<ANNOTATION_VALUE>Todos examinan la explanada, pero no localizan la fuente de la voz.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a15" TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
				<ANNOTATION_VALUE>Poco después camina por la calle, otra vez vestido con el traje.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a14" TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
				<ANNOTATION_VALUE>Recoge su camisa y se la pone.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a31" TIME_SLOT_REF1="ts61" TIME_SLOT_REF2="ts62">
				<ANNOTATION_VALUE>Los tres dan pasos vacilantes y dirigen la vista al cielo.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a30" TIME_SLOT_REF1="ts59" TIME_SLOT_REF2="ts60">
				<ANNOTATION_VALUE>Llega a una gran explanada de arena donde se encuentran James y Rick.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a37" TIME_SLOT_REF1="ts73" TIME_SLOT_REF2="ts74">
				<ANNOTATION_VALUE>Oye, ahora no puedo hablar. Tengo que dejarte.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a36" TIME_SLOT_REF1="ts71" TIME_SLOT_REF2="ts72">
				<ANNOTATION_VALUE>No, ¿de qué me hablas? Sí, claro, las maletas están a punto.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a35" TIME_SLOT_REF1="ts69" TIME_SLOT_REF2="ts70">
				<ANNOTATION_VALUE>No, no he terminado de tenderla. Tengo que darme prisa.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a34" TIME_SLOT_REF1="ts67" TIME_SLOT_REF2="ts68">
				<ANNOTATION_VALUE>Jess corre tras ellos hasta alcanzarlos y se marchan juntos.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a38" TIME_SLOT_REF1="ts75" TIME_SLOT_REF2="ts76">
				<ANNOTATION_VALUE>Zoé. La próxima.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a19" TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
				<ANNOTATION_VALUE>Bebe un trago de café.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a18" TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
				<ANNOTATION_VALUE>Tiene un vaso en las manos y quita la tapa de plástico.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a1" TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
				<ANNOTATION_VALUE>Es un día nublado. En la playa, una pareja pasea a un perro y un hombre hace footing.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a3" TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
				<ANNOTATION_VALUE>Saca unas gafas de sol de un bolsillo y se las pone.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a2" TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
				<ANNOTATION_VALUE>Es un hombre de unos 40 años.  Tiene pelo y barba castaños y viste de traje.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a5" TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
				<ANNOTATION_VALUE>James, en bañador, extiende una toalla en la arena y se tumba.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a4" TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
				<ANNOTATION_VALUE>Es James, el empresario.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a7" TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
				<ANNOTATION_VALUE>Luego extiende los brazos y acaricia la arena.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a6" TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
				<ANNOTATION_VALUE>Se frota los ojos.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a9" TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
				<ANNOTATION_VALUE>Se levanta y y se acerca a la orilla del mar.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
			</ANNOTATION>
		<ANNOTATION>
			<ALIGNABLE_ANNOTATION ANNOTATION_ID="a8" TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
				<ANNOTATION_VALUE>James busca entre sus cosas.</ANNOTATION_VALUE>
				</ALIGNABLE_ANNOTATION>
